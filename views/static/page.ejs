<div class="contentMainContainer">	<div class="interactionsMainContainer chocolat">	<span class="date"><%= moment(page.createdAt).format('dddd D, MMMM YYYY') %><br /></span><div class="clear"></div>	<h1><%= page.name %><a class="visitWebsiteChocolat" title="Visit website" target="_blank" href="http://<%= page.url %>">&nbsp;</a></h1>	<div class="clear"></div>	<h2><span>By</span> <%= page.creator.name %> <span>(<%= page.creator.country %>)</span></h2>	<p><%= page.description %></p>	<p class="allHashtags">	<% _.each(page.iHashtags, function(ihash){ %>		<a href="/chocolat/search/?text=<%= ihash  %>">#<%= ihash %></a>	<% }) %>	</p>	<ul class="listInteractions">		<% _.each(page.interactions,function(item,auxcounter){ %>		<li class="itemInteraction">			<div class="imgContItemInteraction" int="<%= item.id %>">				<% var zIndex=50; %>				<% _.each(item.layers.reverse(), function(layer,counter){ %>				<% if(counter==1){%>					<img style="z-index:<%= (counter%2==0)?counter+1:counter-1 %>;" number="<%= counter %>" class="<%= counter<2?'show bounceV scaleEffect backgroundReset':'' %> <%= layer.type.indexOf('Base')>=0?'blurBelow':'' %>" src="/media/dynamic/<%= page.id %>/image/<%= item.order %>/<%= layer.image  %>" />				<% } %>				<% }) %>			</div>			<div class="textContItemInteraction">				<p class="interactionHashtags">				<% _.each(item.hashtags, function(itemH){ %>						<a href="/chocolat/search/?text=<%= itemH.name  %>">#<%= itemH.name  %></a>					<% }) %>				</p>				<p><%= item.description %></p>			</div>		</li>	<% }) %>	</ul>	<div class="visitWebsiteContainer">		 <a class="visitWebsiteChocolat" title="Visit website" target="_blank" href="http://<%= page.url %>">Visit website</a>	</div>	</div></div>