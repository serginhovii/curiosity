<div class="contentMainContainer">	<div class="searchMainContainer">			<div class="searchMessageResultsContainer">	<h2>Search results</h2>		<p><%= pages.length %> result<%= pages.length!=1?'s':'' %> for <span>'<%= query %>'</span></p>	</div>	<ul class="listHome">		<% _.each(pages,function(item){ %>	<li class="itemHome">		<a class="imgContainerHome"  href="/chocolat/page/<%= item.tag %>">			<img alt='<%= item.name %>' src="/media/dynamic/<%= item.id %>/image/<%= item.interactions[0].order %>/<%= item.layers[1].image  %>" />		</a>		<div class="divItemTextHome">			<h3><a href="/chocolat/page/<%= item.tag %>"><%= item.name %></a></h3>			<p>			By <a href="/chocolat/page/<%= item.tag %>"><%= item.creator.name %></a> from <span><%= item.creator.country %></span><br />			<%= moment(item.createdAt).format('dddd D, MMMM YYYY')  %><br />			</p>		</div>	</li>	<% }) %>	</ul>	</div></div>