<div class="contentMainContainer">	<ul class="listHome">		<% _.each(pages,function(item){ %>	<li class="itemHome">		<a class="imgContainerHome"  href="/page/<%= item.tag %>">			<img alt='<%= item.name %>' src="/media/dynamic/<%= item.id %>/image/<%= item.interactions[0].order %>/<%= item.layers[0].image  %>" />		</a>		<div class="divItemTextHome">			<h3><a href="/page/<%= item.tag %>"><%= item.name %></a></h3>			<p>			By <a href="/page/<%= item.tag %>"><%= item.creator.name %></a> from <span><%= item.creator.country %></span><br />			<%= moment(item.createdAt).format('dddd D, MMMM YYYY')  %><br />			</p>		</div>	</li>	<% }) %>	</ul></div>