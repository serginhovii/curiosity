<div class="contentMainContainer">	<ul class="listHome">		<% _.each(pages,function(item){ %>	<li class="itemHome">		<a class="imgContainerHome">			<img alt='<%= item.name %>' src="/media/dynamic/<%= item.id %>/image/<%= item.layers[0].image  %>" />		</a>		<div class="divItemTextHome">			<h3><a><%= item.name %></a></h3>			<p>			By <a><%= item.creator.name %></a> from <span><%= item.creator.country %></span><br />			<%= item.createdAt %><br />			</p>		</div>	</li>	<% }) %>	</ul>	</div>