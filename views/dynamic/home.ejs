<div class="contentMainContainer">	<ul class="listHome">		<% _.each(pages,function(item){ %>	<li class="itemHome">		<a class="imgContainerHome" style="position:relative;display:block;" href="/page/<%= item.tag %>">			<img alt='<%= item.name %>' src="/media/dynamic/<%= item.id %>/image/<%= item.layers[0].interaction %>/<%= item.layers[0].image  %>" />		<img style="display:block; position:absolute;top:0px;left:0px;" alt='<%= item.name %>' src="/media/dynamic/<%= item.id %>/image/<%= item.layers[1].interaction %>/<%= item.layers[1].image  %>" />		</a>		<div class="divItemTextHome">			<h3><a href="/page/<%= item.tag %>"><%= item.name %></a></h3>			<p>			By <a href="/page/<%= item.tag %>"><%= item.creator.name %></a> from <span><%= item.creator.country %></span><br />			<%= item.createdAt %><br />			</p>		</div>	</li>	<% }) %>	</ul></div>