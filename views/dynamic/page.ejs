<div class="contentMainContainer">	<div class="interactionsMainContainer">	<span class="date"><%= moment(page.createdAt).format('dddd D, MMMM YYYY') %><br /></span><div class="clear"></div>	<h1><%= page.name %></h1>	<h2><span>By</span> <%= page.creator.name %> <span>(<%= page.creator.country %>)</span></h2>	<p><%= page.description %></p>	<p class="allHashtags">	<% _.each(page.iHashtags, function(ihash){ %>		<span>#<%= ihash %></span>	<% }) %>	</p>	<ul class="listInteractions">		<% _.each(page.interactions,function(item,auxcounter){ %>		<li class="itemInteraction">			<div class="imgContItemInteraction" int="<%= item.id %>">				<% _.each(item.layers.reverse(), function(layer,counter){ %>				<img style="z-index:<%= (counter%2==0)?counter+1:counter-1 %>;" number="<%= counter %>" class="<%= counter<2?'show bounceV scaleEffect backgroundReset':'' %> <%= layer.type.indexOf('Base')>=0?'blurBelow':'' %>" src="/media/dynamic/<%= page.id %>/image/<%= item.order %>/<%= layer.image  %>" />				<% }) %>			</div>			<div class="timelineContainer">				<% _.each(item.layers, function(layer,i){ %>					<% if(i%2 == 1){ %>					<a number="<%= i %>" class="layerButton <%= i<2?'show':'' %>"><span class="backEye">&nbsp;</span><%= (i/2)+0.5 %></a>					<% } %>				<% }) %>				<div class="clear"></div>			</div>			<div class="textContItemInteraction">				<!-- <h2><%= auxcounter+2 %></h2> -->				<p class="interactionHashtags">				<% _.each(item.hashtags, function(itemH){ %>						<span>#<%= itemH.name  %></span>					<% }) %>				</p>				<p><%= item.description %></p>			</div>		</li>	<% }) %>	</ul>	</div></div>