<div class="contentMainContainer">	<!--	<%= JSON.stringify(page.designtags) %>	-->	<ul class="listInteractions">		<% _.each(interactions,function(item){ %>	<li class="itemInteraction">		<!--		<%= JSON.stringify(item.hashtags) %>		<%= JSON.stringify(item.layers) %>		-->		<div class="imgContItemInteraction">		<img src="/media/dynamic/<%= page.id %>/image/<%= item.id %>/<%= item.layers[0].image  %>" />		</div>		<div class="textContItemInteraction">			<p><%= item.description %></p>		</div>	</li>	<% }) %>	</ul></div>