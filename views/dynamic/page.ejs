<div class="contentMainContainer">	<div class="interactionsMainContainer">	<span class="date"><%= moment(page.createdAt).format('dddd D, MMMM YYYY') %><br /></span>	<h1><%= page.name %></h1>	<h2><span>By</span> <%= page.creator.name %> <span>(<%= page.creator.country %>)</span></h2>	<p><%= page.description %></p>	<p class="allHashtags">	<% _.each(page.iHashtags, function(ihash){ %>		<span>#<%= ihash %></span>	<% }) %>	</p>	<ul class="listInteractions">		<% _.each(page.interactions,function(item){ %>		<li class="itemInteraction">			<div class="imgContItemInteraction" int="<%= item.id %>">				<% var zIndex=50; %>				<% _.each(item.layers.reverse(), function(layer,counter){ %>				<img number="<%= layer.id %>" class="<%= layer.type.indexOf('Event')>=0?'show bounceV scaleEffect backgroundReset':'' %> <%= layer.type.indexOf('Base')>=0?'blurBelow':'' %>" src="/media/dynamic/<%= page.id %>/image/<%= item.order %>/<%= layer.image  %>" />				<% }) %>			</div>			<div class="timelineContainer">				<% _.each(item.layers.reverse(), function(layer){ %>					<a number="<%= layer.id %>" class="layerButton <%= layer.type.indexOf('Event')>=0?'show':'' %>"><span class="backEye">&nbsp;</span><%= layer.type %></a>				<% }) %>			</div>			<div class="textContItemInteraction">				<p class="interactionHashtags">				<% _.each(item.hashtags, function(itemH){ %>						<span>#<%= itemH.name  %></span>					<% }) %>				</p>				<p><%= item.description %></p>			</div>		</li>	<% }) %>	</ul>	</div></div>